<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CatatanKajian"
    android:orientation="vertical"
    android:paddingBottom="@dimen/nilaiPadding"
    android:paddingLeft="@dimen/nilaiPadding"
    android:paddingRight="@dimen/nilaiPadding"
    android:paddingTop="@dimen/nilaiPadding"
    >

    <TextView
        android:id="@+id/catatanKajian"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CATATAN KAJIAN" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</RelativeLayout>